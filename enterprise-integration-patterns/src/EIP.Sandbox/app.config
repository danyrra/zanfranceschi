<?xml version="1.0"?>
<configuration>
	<appSettings>
		<add key="queue" value=".\private$\TestQueue"/>
	</appSettings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
 <system.serviceModel>
  <bindings>
   <basicHttpBinding>
    <binding name="BasicHttpBinding_IServiceRegistry" closeTimeout="00:01:00"
     openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"
     allowCookies="false" bypassProxyOnLocal="false" maxBufferPoolSize="524288"
     maxReceivedMessageSize="65536" useDefaultWebProxy="true" />
   </basicHttpBinding>
   <netTcpBinding>
    <binding name="NetTcpBinding_IServiceRegistry" closeTimeout="00:01:00"
     openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"
     maxBufferPoolSize="524288" maxReceivedMessageSize="65536">
     <security mode="None" />
    </binding>
   </netTcpBinding>
   <wsHttpBinding>
    <binding name="WSHttpBinding_IServiceRegistry" closeTimeout="00:01:00"
     openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"
     bypassProxyOnLocal="false" maxBufferPoolSize="524288" maxReceivedMessageSize="65536"
     useDefaultWebProxy="true" allowCookies="false">
     <security mode="None" />
    </binding>
   </wsHttpBinding>
  </bindings>
  <client>
   <!--<endpoint address="http://localhost:8088/service-registry/http"
    binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceRegistry"
    contract="ServiceReference1.IServiceRegistry" name="BasicHttpBinding_IServiceRegistry" />
   <endpoint address="net.tcp://localhost:8098/service-registry/tcp"
    binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IServiceRegistry"
    contract="ServiceReference1.IServiceRegistry" name="NetTcpBinding_IServiceRegistry" />-->
   <endpoint address="http://localhost:8088/service-registry" binding="wsHttpBinding"
    bindingConfiguration="WSHttpBinding_IServiceRegistry" contract="ServiceReference1.IServiceRegistry"
    name="WSHttpBinding_IServiceRegistry" />
  </client>
 </system.serviceModel>
 </configuration>
