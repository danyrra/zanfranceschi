<?xml version="1.0"?>
<configuration>
	<appSettings>
		<add key="baseWebAddress" value="http://localhost:8080/service-registry"/>
		<add key="baseTcpAddress" value="net.tcp://localhost:8090/service-registry"/>
	</appSettings>
	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="httpBinding"/>
			</basicHttpBinding>
			<netTcpBinding>
				<binding name="tcpBinding">
					<security mode="None"/>
				</binding>
			</netTcpBinding>
			<wsHttpBinding>
				<binding name="wsBinding">
					<security mode="None"/>
				</binding>
			</wsHttpBinding>
		</bindings>
		<services>
			<service name="EIP.ServiceRegistry.Host._Service.IServiceRegistry">
				<endpoint binding="wsHttpBinding" bindingConfiguration="wsBinding" name="wsEndpoint" contract="EIP.ServiceRegistry.Host._Service.IServiceRegistry" listenUriMode="Explicit" />
				<endpoint address="mex" binding="mexHttpBinding" name="mexEndPoint" contract="IMetadataExchange" listenUriMode="Explicit" />
				<endpoint address="http" binding="basicHttpBinding" bindingConfiguration="httpBinding" name="httpEndPoint" contract="EIP.ServiceRegistry.Host._Service.IServiceRegistry" listenUriMode="Explicit" />
				<endpoint address="tcp" binding="netTcpBinding" bindingConfiguration="tcpBinding" name="tcpEndPoint" contract="EIP.ServiceRegistry.Host._Service.IServiceRegistry"/>
			</service>
		</services>
	</system.serviceModel>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
	</startup>
</configuration>
